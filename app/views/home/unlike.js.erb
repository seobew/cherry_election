if(<%==@login %> == true){
  if(<%=@is_cancel%> == true){
    $('.unlike_form_<%=@article_id%> #is_cancel').val("false");
    $('.unlike_form_<%=@article_id%> .article_unlike_<%=@article_id%>').text("(<%=@unlike_num%>)");
    $('.unlike_form_<%=@article_id%> input[type="image"]').removeClass("brightness");
  }
  else{
    $('.unlike_form_<%=@article_id%> #is_cancel').val("true");
    $('.unlike_form_<%=@article_id%> .article_unlike_<%=@article_id%>').text("(<%=@unlike_num%>)");
    $('.unlike_form_<%=@article_id%> input[type="image"]').addClass("brightness");

    // 좋아요 취소로 되어있으면(좋아요가 눌러있으면) 싫어요로 바꿔주기
    if(<%=@like_check %>==true){
      $('.like_form_<%=@article_id%> #is_cancel').val("false");
      $('.like_form_<%=@article_id%> .article_like_<%=@article_id%>').text("(<%=@like_num%>)");
      $('.like_form_<%=@article_id%> input[type="image"]').removeClass("brightness");
    }
  }
}
else{
  alert('로그인 후에 이용해주세요!');
}
