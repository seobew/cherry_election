if(<%==@login %> == true){
  if(<%=@is_cancel%> == true){
    $('.unlike_form_<%=@article_id%> #is_cancel').val("false");
    $('.unlike_form_<%=@article_id%> input[type="submit"]').val("싫어요(<%=@unlike_num%>)");
  }
  else{
    $('.unlike_form_<%=@article_id%> #is_cancel').val("true");
    $('.unlike_form_<%=@article_id%> input[type="submit"]').val("싫어요 취소(<%=@unlike_num%>)");

    // 좋아요 취소로 되어있으면(좋아요가 눌러있으면) 싫어요로 바꿔주기
    if(<%=@like_check %>==true){
      $('.like_form_<%=@article_id%> #is_cancel').val("false");
      $('.like_form_<%=@article_id%> input[type="submit"]').val("좋아요(<%=@like_num%>)");
    }
  }
}
else{
  alert('로그인 후에 이용해주세요!');
}
